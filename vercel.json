{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "app.py"
        }
    ],
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "buildCommand": "pip uninstall -y asyncio || true"
}